        <ImageView
            android:id="@+id/edt2"
            android:layout_width="match_parent"

            android:layout_height="200dp" />

        <EditText
            android:id="@+id/edt3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="tombol" />
  <Button
            android:id="@+id/btnshow"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="tombol" />

    </LinearLayout>
</layout>

